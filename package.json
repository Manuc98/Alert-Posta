{
  "name": "alertapostas-worker",
  "version": "1.0.0",
  "description": "Alert@Postas Cloudflare Worker - Proxy e cache para API principal",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:clean": "wrangler deploy --config wrangler.clean.toml",
    "deploy:free": "wrangler deploy --config wrangler.free.toml --env free",
    "deploy:production": "wrangler deploy --env production",
    "fix:durable": "node -e \"console.log('Use: fix-durable-objects.bat (Windows) or ./fix-durable-objects.sh (Linux/Mac)')\"",
    "fix:migration": "node -e \"console.log('Use: fix-migration.bat (Windows) or ./fix-migration.sh (Linux/Mac)')\"",
    "deploy:migration": "wrangler deploy --config wrangler.migration.toml",
    "deploy:simple": "wrangler deploy --config wrangler.simple.toml",
    "delete:old": "wrangler delete alertapostas --force && wrangler delete alertapostas-new --force && wrangler delete alertapostas-v3 --force",
    "build": "echo 'No build step required for this worker'",
    "test": "echo 'No tests configured'",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "cloudflare-worker",
    "alertapostas",
    "proxy",
    "cache",
    "api"
  ],
  "author": "Alert@Postas Team",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250917.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "wrangler": "^4.37.1"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
